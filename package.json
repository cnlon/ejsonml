{
  "name": "ejsonml",
  "version": "0.0.4",
  "description": "JsonML with expression",
  "main": "dist/ejsonml.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "$(npm bin)/eslint render/** parser/**",
    "dev": "$(npm bin)/webpack-dev-server --config dev/webpack.dev.config.js --content-base dev/ --inline --hot --port 3035",
    "watch": "cd dev && node ../parser/bin.js -c",
    "build": "rm -rf dist/* && npm run rollup && MIN=min npm run rollup",
    "rollup": "$(npm bin)/rollup --config --environment NODE_ENV:production --sourcemap"
  },
  "bin": {
    "ejsonml": "parser/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lon3/ejsonml.git"
  },
  "keywords": [
    "jsonml",
    "expression",
    "ejsonml"
  ],
  "author": "longhao he <longhaohe@gmail.com> (http://longhaohe.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lon3/ejsonml/issues"
  },
  "homepage": "https://github.com/lon3/ejsonml#readme",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.0",
    "eslint-plugin-standard": "^1.3.2",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-uglify": "^0.3.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "ejsonml-parser": "^0.0.5",
    "ejsonml-parser-javascript": "^0.0.3",
    "ejsonml-render": "^0.0.4"
  }
}
